<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
        {{ encore_entry_link_tags('kimouss/styleguide') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('app') }}
        {{ encore_entry_script_tags('kimouss/styleguide') }}
    {% endblock %}
</head>
<body>
{% include 'partials/_sidebar.html.twig' %}
<div class="km-layout_sidebar">
    <div class="km-layout_wrapper">
        <div {{ stimulus_controller('symfony/ux-swup/swup') }} id="swup" data-turbo="false">
            <div class="km-layout_content">
                <div class="km-container">
                    {% block body %}{% endblock %}
                </div>
            </div>
        </div>
        {% include 'partials/_footer.html.twig' %}
    </div>
</div>
</body>
</html>
